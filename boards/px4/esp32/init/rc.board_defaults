param set-default RC_MAP_FLTMODE    5
param set-default RC_MAP_ARM_SW     6
param set-default RC3_TRIM 1000

param set-default CAL_ACC0_ID 3538970
param set-default CAL_ACC0_PRIO 50
param set-default CAL_ACC0_TEMP 51.5979
param set-default CAL_ACC0_XOFF 0.0405
param set-default CAL_ACC0_XSCALE 1.0003
param set-default CAL_ACC0_YOFF 0.2861
param set-default CAL_ACC0_YSCALE 1.0008
param set-default CAL_ACC0_ZOFF -0.4584
param set-default CAL_ACC0_ZSCALE 0.9875
param set-default CAL_GYRO0_ID 3538970
param set-default CAL_GYRO0_PRIO 50
param set-default CAL_GYRO0_XOFF 0.0112
param set-default CAL_GYRO0_YOFF -0.0144
param set-default CAL_GYRO0_ZOFF 0.0043
param set-default CAL_MAG0_ID 73217
param set-default CAL_MAG0_PRIO 50
param set-default CAL_MAG0_ROT -1
param set-default CAL_MAG0_TEMP -1000.0000
param set-default CAL_MAG0_XCOMP 0.0000
