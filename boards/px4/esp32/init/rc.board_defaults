param set-default BAT1_SOURCE 0
param set-default BAT1_N_CELLS 1
param set-default BAT1_V_DIV 2.1998
param set-default BAT1_A_PER_V 36.36751556

param set-default SYS_AUTOSTART 4800
param set-default MAV_TYPE 2
param set-default EKF2_MULTI_IMU 0
param set-default SENS_IMU_MODE 1
param set-default EKF2_PREDICT_US 30000
param set-default IMU_INTEG_RATE 100
